<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.printer.HelloController">
   <children>
      <Button fx:id="print" layoutX="78.0" layoutY="233.0" onAction="#onHelloButtonClick" prefHeight="46.0" prefWidth="109.0" style="-fx-background-color: #0598ff;" text="Print" textFill="WHITE">
         <font>
            <Font size="21.0" />
         </font></Button>
      <Button fx:id="closeButton" layoutX="562.0" layoutY="14.0" mnemonicParsing="false" onAction="#onCloseButtonClick" text="X" />
      <Label fx:id="welcomeText" layoutX="44.0" layoutY="36.0" prefHeight="0.0" prefWidth="0.0" />
      <ComboBox fx:id="comboBox" layoutX="26.0" layoutY="110.0" onAction="#onComboBoxSelect" prefHeight="26.0" prefWidth="214.0" promptText="Choose Printer" style="-fx-background-color: transparent; -fx-border-color: #0598ff; -fx-border-width: 0px 0px 2px 0px;" />
      <Label layoutX="249.0" layoutY="17.0" prefHeight="38.0" prefWidth="101.0" text="Printer">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Label>
      <ListView fx:id="listView" layoutX="283.0" layoutY="125.0" prefHeight="262.0" prefWidth="302.0" style="-fx-border-color: #0598ff;" />
      <Label layoutX="352.0" layoutY="83.0" text="Choose Files to print">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Label fx:id="selection" layoutX="119.0" layoutY="343.0" />
   </children>
</AnchorPane>
